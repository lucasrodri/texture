{
  "name": "Texture",
  "version": "${version}",
  "description": "A scientific word processor.",
  "main": "main.js",
  "dependencies": ${dependencies},
  "build": {
    "electronVersion": "${electronVersion}",
    "appId": "com.texture.texture-desktop",
    "copyright": "Copyright Â© 2018 Substance",
    "files": [
      "data",
      "lib",
      "node_modules",
      "templates",
      "examples",
      "*.html",
      "*.js",
      "package.json"
    ],
    "asar": "false",
    "directories": {
      "output": "build",
      "buildResources": "build-resources"
    },
    "publish": null,
    "fileAssociations": [{
      "ext": "dar",
      "name": "Document Archive",
      "role": "Editor"
    }]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/substance/texture.git"
  },
  "license": "MIT"
}
